generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Appointment {
  id         String    @id @default(uuid())
  customer   String
  provider   String
  startAt    DateTime
  endAt      DateTime
  createdAt  DateTime  @default(now())
  canceledAt DateTime?
}
